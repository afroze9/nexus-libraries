<Project>
    <PropertyGroup>
        <NexusLibraryVersion>0.2.0.0</NexusLibraryVersion>
    </PropertyGroup>
    <PropertyGroup>
        <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
        <ImplicitUsings>enable</ImplicitUsings>
        <NoWarn>$(NoWarn);1591</NoWarn>
        <Nullable>enable</Nullable>
        <OutputPath>$(SolutionDir)\artifacts\$(Configuration)\</OutputPath>
        <TargetFramework>net7.0</TargetFramework>
    </PropertyGroup>
    <PropertyGroup>
        <AssemblyVersion>$(NexusLibraryVersion)</AssemblyVersion>
        <Authors>afroze9</Authors>
        <DebugType>embedded</DebugType>
        <FileVersion>$(NexusLibraryVersion)</FileVersion>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/afroze9/nexus</PackageProjectUrl>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/afroze9/nexus</RepositoryUrl>
        <Version>$(NexusLibraryVersion)</Version>
    </PropertyGroup>
</Project>
